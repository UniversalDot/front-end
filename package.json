{
  "name": "@minimal/material-kit-react",
  "author": "minimals.cc",
  "version": "3.4.0",
  "description": "Simple React Scripts & Typescript",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build-interfaces": "npm run generate:defs && npm run generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package universaldot-frontend/interfaces --input ./src/interfaces --endpoint ./src/interfaces/metadata.json",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package universaldot-frontend/interfaces --endpoint ./src/interfaces/metadata.json --output ./src/interfaces",
    "eject": "react-scripts eject",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@iconify/react": "^3.2.1",
    "@mui/material": "^5.6.0",
    "@polkadot/api": "^7.10.1",
    "@polkadot/extension-dapp": "^0.42.7",
    "@polkadot/keyring": "^8.4.1",
    "@polkadot/networks": "^8.4.1",
    "@polkadot/types": "^8.11.3",
    "@polkadot/ui-keyring": "^1.1.1",
    "@polkadot/ui-settings": "^1.1.1",
    "@polkadot/util": "^8.7.1",
    "@polkadot/util-crypto": "^8.4.1",
    "@reduxjs/toolkit": "^1.8.1",
    "change-case": "^4.1.2",
    "console-browserify": "^1.2.0",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.2.9",
    "history": "^5.3.0",
    "node-polyfill-webpack-plugin": "^2.0.0",
    "notistack": "^2.0.3",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-lazy-load-image-component": "^1.5.4",
    "react-redux": "^7.2.8",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.0",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "simplebar": "^5.3.6",
    "simplebar-react": "^2.3.6",
    "stream-browserify": "^3.0.0",
    "stylis": "^4.1.0",
    "stylis-plugin-rtl": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@polkadot/typegen": "^8.11.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-redux": "^7.1.23",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.2.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.5.5"
  }
}
